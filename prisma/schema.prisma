generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  bio       String?
  create_on DateTime
  email     String   @unique
  id        String   @id
  image     String?
  password  String
  social    String[]
  username  String
  website   String?
}